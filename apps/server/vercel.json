{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"functions": {
		"api/index.ts": {
			"runtime": "nodejs20.x",
			"memory": 1024,
			"maxDuration": 10
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/api/index.ts"
		}
	],
	"installCommand": "curl -fsSL https://bun.sh/install | bash && ~/.bun/bin/bun install",
	"buildCommand": "~/.bun/bin/bun run build"
}
